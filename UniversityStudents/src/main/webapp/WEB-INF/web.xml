<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>StudentsServlet</servlet-name>
  	<display-name>StudentsServlet</display-name>
  	<description></description>
  	<servlet-class>com.uni.StudentsServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SaveRecord</servlet-name>
  	<display-name>SaveRecord</display-name>
  	<description></description>
  	<servlet-class>com.uni.SaveRecord</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Ser</servlet-name>
  	<display-name>Ser</display-name>
  	<description></description>
  	<servlet-class>com.uni.Ser</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>StudentsServlet</servlet-name>
  	<url-pattern>/students</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SaveRecord</servlet-name>
  	<url-pattern>/save</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Ser</servlet-name>
  	<url-pattern>/Ser</url-pattern>
  </servlet-mapping>
</web-app>
